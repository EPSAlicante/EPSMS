^ Signature | {{ proc["Signature"]|default("") }} |
^ ID | {{ proc["ID"]|default("") }} |
^ Version | {{ proc["Version"]|default("") }} |
^ Voltage | {{ proc["Voltage"]|default("") }} |
^ External Clock | {{ proc["External Clock"]|default("") }} |
^ Max Speed | {{ proc["Max Speed"]|default("") }} |
^ Current Speed | {{ proc["Current Speed"]|default("") }} |
^ Status | {{ proc["Status"]|default("") }} |
^ Upgrade | {{ proc["Upgrade"]|default("") }} |
^ L1 Cache Handle | {{ "[[ %s-caches | %s ]]" % (inventory_hostname,proc["L1 Cache Handle"]|default("")) }} |
^ L2 Cache Handle | {{ "[[ %s-caches | %s ]]" % (inventory_hostname,proc["L2 Cache Handle"]|default("")) }} |
^ L3 Cache Handle | {{ "[[ %s-caches | %s ]]" % (inventory_hostname,proc["L3 Cache Handle"]|default("")) }} |
^ Serial Number | {{ proc["Serial Number"]|default("") }} |
{% if proc["Flags"] is defined -%}
^  Characteristics  ^^
{% for char, value in proc["Flags"].iteritems() -%}
^ {{ char|e }} | {{ value|e }} |
{% endfor %}
